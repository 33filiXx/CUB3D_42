										{
											"type": "paragraph",
											"attrs": {
												"dir": "auto"
											},
											"content": [
												{
													"type": "text",
													"text": "  = (map_x - px + (1 - step_x)/2) / ray_dir_x"
												}
											]
										}
									]
								}
							]
						},
						{
							"type": "paragraph",
							"attrs": {
								"dir": "auto"
							},
							"content": [
								{
									"type": "text",
									"text": "So the "
								},
								{
									"type": "text",
									"marks": [
										{
											"type": "code"
										}
									],
									"text": "(1 - step_x)/2"
								},
								{
									"type": "text",
									"text": " term is nothing mysterious—it’s just a compact algebraic way to add “+1 when stepping left, +0 when stepping right.”"
								}
							]
						}
					]
