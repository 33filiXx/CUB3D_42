															},
															"content": [
																{
																	"type": "text",
																	"text": "The formula "
																},
																{
																	"type": "text",
																	"marks": [
																		{
																			"type": "code"
																		}
																	],
																	"text": "boundary_x = map_x + (1 − step_x) / 2"
																},
																{
																	"type": "text",
																	"text": " captures both cases:"
																}
															]
														},
														{
															"type": "bulletList",
															"content": [
																{
																	"type": "listItem",
																	"content": [
																		{
																			"type": "paragraph",
