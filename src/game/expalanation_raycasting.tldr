													"type": "text",
													"marks": [
														{
															"type": "code"
														}
													],
													"text": "step_x = 1"
												},
												{
													"type": "text",
													"text": "), or "
												},
												{
													"type": "text",
													"marks": [
														{
															"type": "code"
														}
													],
													"text": "map_x + 1"
												},
												{
													"type": "text",
													"text": " if we stepped left ("
												},
												{
													"type": "text",
													"marks": [
														{
